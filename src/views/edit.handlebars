<form method="post">
  <label for="id">ID:</label>
  <input type="number" id="id" name="id" placeholder="{{club.id}}" />

  <label for="areaId">Area ID:</label>
  <input type="text" id="areaId" name="areaId" placeholder="{{area.id}}" />

  <label for="areaName">Area Name:</label>
  <input
    type="text"
    id="areaName"
    name="areaName"
    placeholder="{{area.name}}"
  />

  <label for="name">Name:</label>
  <input type="text" id="name" name="name" placeholder="{{club.name}}" />

  <label for="shortName">Short Name:</label>
  <input
    type="text"
    id="shortName"
    name="shortName"
    placeholder="{{club.shortName}}"
  />

  <label for="tla">TLA:</label>
  <input type="text" id="tla" name="tla" placeholder="{{club.tla}}" />

  <label for="crestUrl">Crest URL:</label>
  <input
    type="text"
    id="crestUrl"
    name="crestUrl"
    placeholder="{{club.crestUrl}}"
  />

  <label for="address">Address:</label>
  <input
    type="text"
    id="address"
    name="address"
    placeholder="{{club.address}}"
  />

  <label for="phone">Phone:</label>
  <input type="text" id="phone" name="phone" placeholder="{{club.phone}}" />

  <label for="website">Website:</label>
  <input
    type="text"
    id="website"
    name="website"
    placeholder="{{club.website}}"
  />

  <label for="email">Email:</label>
  <input type="text" id="email" name="email" placeholder="{{club.email}}" />

  <label for="founded">Founded:</label>
  <input
    type="number"
    id="founded"
    name="founded"
    placeholder="{{club.founded}}"
  />

  <label for="clubColors">Club Colors:</label>
  <input
    type="text"
    id="clubColors"
    name="clubColors"
    placeholder="{{club.clubColors}}"
  />

  <label for="venue">Venue:</label>
  <input type="text" id="venue" name="venue" placeholder="{{club.venue}}" />

  <label for="lastUpdated">Last Updated:</label>
  <input
    type="text"
    id="lastUpdated"
    name="lastUpdated"
    placeholder="{{club.lastUpdated}}"
  />

  <input type="submit" value="Submit" />
</form>
<button onclick="location.href='http://localhost:8080/'">home</button>