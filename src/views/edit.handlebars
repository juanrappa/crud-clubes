<form action="/{{club.id}}/edit" method="POST" enctype="multipart/form-data">
  <label for="id">ID:</label>
  <input type="number" id="id" name="id" value="{{club.id}}" />

  <label for="areaId">Area ID:</label>
  <input type="text" id="areaId" name="areaId" value="{{club.area.id}}" />

  <label for="areaName">Area Name:</label>
  <input type="text" id="areaName" name="areaName" value="{{club.area.name}}" />

  <label for="name">Name:</label>
  <input type="text" id="name" name="name" value="{{club.name}}" />

  <label for="shortName">Short Name:</label>
  <input
    type="text"
    id="shortName"
    name="shortName"
    value="{{club.shortName}}"
  />

  <label for="tla">TLA:</label>
  <input type="text" id="tla" name="tla" value="{{club.tla}}" />

  <label for="crestUrl">Crest URL:</label>
  <input
    type="file"
    id="crestUrl"
    name="crestUrl"
    accept=".jpg"
    value="{{club.crestUrl}}"
  />

  <label for="address">Address:</label>
  <input type="text" id="address" name="address" value="{{club.address}}" />

  <label for="phone">Phone:</label>
  <input type="text" id="phone" name="phone" value="{{club.phone}}" />

  <label for="website">Website:</label>
  <input type="text" id="website" name="website" value="{{club.website}}" />

  <label for="email">Email:</label>
  <input type="text" id="email" name="email" value="{{club.email}}" />

  <label for="founded">Founded:</label>
  <input type="number" id="founded" name="founded" value="{{club.founded}}" />

  <label for="clubColors">Club Colors:</label>
  <input
    type="text"
    id="clubColors"
    name="clubColors"
    value="{{club.clubColors}}"
  />

  <label for="venue">Venue:</label>
  <input type="text" id="venue" name="venue" value="{{club.venue}}" />

  <label for="lastUpdated">Last Updated:</label>
  <input
    type="text"
    id="lastUpdated"
    name="lastUpdated"
    value="{{club.lastUpdated}}"
  />

  <input type="submit" value="Submit" />
</form>
<button onclick="location.href='http://localhost:8080/'">home</button>