<form action="/create" method="POST" enctype="multipart/form-data">
  <label for="id">ID:</label>
  <input type="number" id="id" name="id" value="{{id}}" />

  <label for="areaId">Area ID:</label>
  <input type="text" id="areaId" name="areaId" value="{{areaId}}" />

  <label for="areaName">Area Name:</label>
  <input type="text" id="areaName" name="areaName" value="{{areaName}}" />

  <label for="name">Name:</label>
  <input type="text" id="name" name="name" value="{{name}}" />

  <label for="shortName">Short Name:</label>
  <input type="text" id="shortName" name="shortName" value="{{shortName}}" />

  <label for="tla">TLA:</label>
  <input type="text" id="tla" name="tla" value="{{tla}}" />

  <label for="crestUrl">Crest URL:</label>
  <input
    type="file"
    id="crestUrl"
    name="crestUrl"
    accept=".jpg"
    value="{{crestUrl}}"
  />

  <label for="address">Address:</label>
  <input type="text" id="address" name="address" value="{{address}}" />

  <label for="phone">Phone:</label>
  <input type="text" id="phone" name="phone" value="{{phone}}" />

  <label for="website">Website:</label>
  <input type="text" id="website" name="website" value="{{website}}" />

  <label for="email">Email:</label>
  <input type="text" id="email" name="email" value="{{email}}" />

  <label for="founded">Founded:</label>
  <input type="number" id="founded" name="founded" value="{{founded}}" />

  <label for="clubColors">Club Colors:</label>
  <input type="text" id="clubColors" name="clubColors" value="{{clubColors}}" />

  <label for="venue">Venue:</label>
  <input type="text" id="venue" name="venue" value="{{venue}}" />

  <label for="lastUpdated">Last Updated:</label>
  <input
    type="text"
    id="lastUpdated"
    name="lastUpdated"
    value="{{lastUpdate}}"
  />

  <input type="submit" value="Submit" />
</form>
<button onclick="location.href='http://localhost:8080/'">home</button>